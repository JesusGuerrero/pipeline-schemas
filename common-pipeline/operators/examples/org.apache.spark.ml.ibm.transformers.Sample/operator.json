{
  "id": "org.apache.spark.ml.ibm.transformers.Sample",
  "version": "1.0",
  "operator_type": "transformer",
  "input_ports": [
    {
      "id": "input_port",
      "cardinality": {
        "min": 1,
        "max": 1
      }
    }
  ],
  "output_ports": [
    {
      "id": "output_port",
      "cardinality": {
        "min": 1,
        "max": -1
      }
    }
  ],
  "parameters": [
    {
      "id": "withReplacement",
      "type": "boolean",
      "default": false,
      "required": true
    },
    {
      "id": "samplingSize",
      "type": "long",
      "default": -1,
      "required": true
    },
    {
      "id": "samplingRatio",
      "type": "double",
      "default": 1,
      "required": true
    },
    {
      "id": "samplingSeed",
      "type": "integer",
      "default": -1,
      "required": false
    }
  ]
}
