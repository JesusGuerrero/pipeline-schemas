{
  "name":"org.apache.spark.ml.classification.GBTClassifier",
  "label":"Gradient Boosted Tree Classifier",
  "description":"Fit a Gradient Boosted Tree Classifier Model",
  "operator_type":"estimator",
  "categories":[
    "canvas-modeling",
    "estimator",
    "training"
  ],
  "scoringTransformer":"org.apache.spark.ml.classification.GBTClassificationModel",
  "context":"dataset",
  "parameters":[
    {
      "name":"targetField",
      "type":"string",
      "default":"",
      "role":"column",
      "required":true
    },
    {
      "name":"inputFieldList",
      "type":"array[string]",
      "default":[],
      "role":"column",
      "required":true
    },
    {
      "name":"checkpointInterval",
      "type":"integer",
      "default":-1
    },
    {
      "name":"impurity",
      "enum":[
        "gini",
        "entropy"
      ],
      "default":"gini"
    },
    {
      "name":"lossType",
      "type":"string",
      "default":"logistic"
    },
    {
      "name":"maxBins",
      "type":"integer",
      "default":32
    },
    {
      "name":"maxDepth",
      "type":"integer",
      "default":5
    },
    {
      "name":"maxIter",
      "type":"integer",
      "default":100
    },
    {
      "name":"minInfoGain",
      "type":"double",
      "default":0.0
    },
    {
      "name":"minInstancesPerNode",
      "type":"integer",
      "default":1
    },
    {
      "name":"seed",
      "type":"integer",
      "default":12345
    },
    {
      "name":"stepSize",
      "type":"double",
      "default":0.0
    },
    {
      "name":"subsamplingRate",
      "type":"double",
      "default":1.0
    }
  ]
}
